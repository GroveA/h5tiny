{
  "name": "h5tiny",
  "version": "1.2.9",
  "description": "Fast and tiny JavaScript library for HTML5 game and creatives developing.",
  "main": "./lib/tiny.js",
  "directories": {
    "src": "src",
    "three": "three",
    "tiny": "tiny",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-hutsul/h5tiny"
  },
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "prod": "gulp",
    "build:cleanup": "rm -rf lib",
    "build:webpack": "webpack",
    "build:examples": "cpx 'examples/**/*' dist",
    "build:examplesLib": "cpx 'lib/**/*' dist",
    "watch": "npm-run-all --parallel watch:*",
    "watch:cleanup": "rm -rf lib",
    "watch:webpack": "webpack --watch",
    "watch:examples": "cpx 'examples/**/*' dist --watch",
    "watch:examplesLib": "cpx 'lib/**/*' dist --watch"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.0.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "^1.0.8",
    "gulp-babel": "^8.0.0",
    "gulp-buffer": "0.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-strip-debug": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "vinyl-source-stream": "^2.0.0",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-merge-and-include-globally": "^2.1.20"
  },
  "keywords": [
    "2d",
    "HTML5",
    "canvas",
    "game",
    "tiny",
    "javascript"
  ],
  "author": "Green Panda Games",
  "license": "ISC"
}
